[
{rabbit, [
   {ssl_listeners, [5671]},
   {ssl_options, [{cacertfile,"/etc/rabbitmq/ca_cert.pem"},
                  {certfile,"/etc/rabbitmq/rabbitmq_cert.pem"},
                  {keyfile,"/etc/rabbitmq/rabbitmq_key.pem"},
                  {verify, verify_peer},
                  {fail_if_no_peer_cert, true}]}
 ]}
